<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
	    <MtouchExtraArgs>--dlsym:+nunit.framework.dll $(MtouchExtraArgs)</MtouchExtraArgs>
	</PropertyGroup>

	<ItemGroup>
		<LinkDescription Include="$(MSBuildThisFileDirectory)nunit.framework.xml" />
	</ItemGroup>
</Project>
